    <script>
  var autoMoveIntervalMain = null
      mainTopSlider = null,
      mainSmallSlider = null,
      slideInterval = 6000,
      sliderWidthThreshold = 970,
      siemaTop = document.querySelector('.siema-main'),
      siemaSmall = document.querySelector('.siema-small');

  function startAutoMoveMain() {
    clearInterval(autoMoveIntervalMain);
    autoMoveIntervalMain = setInterval(function() {
        mainTopSlider.next();
        mainSmallSlider.next();
      }, slideInterval);
  }

  if (typeof debounce !== 'function') {
    function debounce(func, wait, immediate) {
      var timeout;
      return function () {
        var args = arguments;
        var later = function () {
          timeout = null;
          if (!immediate) func.apply(this, args);
        }
        var callNow = immediate && !timeout;
        clearTimeout(timeout);
        timeout = setTimeout(later, wait);
        if (callNow) func.apply(this, args);
      }
    }
  }

  // Init sliders
  function createSliders() {
    if (mainTopSlider === null) {
      mainTopSlider = new window.Siema({
        selector: '.siema-main',
        loop: true,
        perPage: 2,
        duration: 2000,
        easing: 'ease',
        draggable: false,
        multipleDrag: false,
        onInit: function() {
          document.querySelector('.siema-main').classList.remove('nojs');
          startAutoMoveMain();
        }
      });
    }

    if (mainSmallSlider === null) {
      mainSmallSlider = new window.Siema({
        selector: '.siema-small',
        loop: true,
        perPage: 3,
        startIndex: 2,
        duration: 2000,
        easing: 'ease',
        draggable: false,
        multipleDrag: false,
        onInit: function() {
          document.querySelector('.siema-small').classList.remove('nojs');
        }
      });
    }
  }

  function destroySliders() {
    if (mainTopSlider !== null) {
      mainTopSlider.destroy(true);
      document.querySelector('.siema-main').classList.add('nojs');
      mainTopSlider = null
    }
    if (mainSmallSlider !== null) {
      mainSmallSlider.destroy(true);
      document.querySelector('.siema-small').classList.add('nojs');
      mainSmallSlider = null
    }
  }

  window.addEventListener('load', function() {
    // Wait until all resources have been loaded
    // Then check if the user is an admin
    // If not init the slider
    // Otherwise just skip the slider entirely
    setTimeout(function () {
      if (document.body.classList.contains('user-editor')) {
        return;
      }
      // Stop interval when a user hovers on the slider
      siemaTop.addEventListener('mouseenter', function() {
        clearInterval(autoMoveIntervalMain);
      });
      // Start interval when a user leaves the slider
      siemaTop.addEventListener('mouseleave', function() {
        startAutoMoveMain();
      });
      // Stop interval when a user hovers on the slider
      siemaSmall.addEventListener('mouseenter', function() {
        clearInterval(autoMoveIntervalMain);
      });
      // Start interval when a user leaves the slider
      siemaSmall.addEventListener('mouseleave', function() {
        startAutoMoveMain();
      });

      // Create sliders if width is sufficient
      if (window.innerWidth >= sliderWidthThreshold) {
        createSliders();
      }

      // Check slider on resize events
      window.addEventListener('resize', debounce(function() {
        var innerWidthAfterResize = window.innerWidth;
        if (innerWidthAfterResize >= sliderWidthThreshold) {
          createSliders();
        } else {
          // Remove interval
          clearInterval(autoMoveIntervalMain);
          // Remove sliders and restore markup
          destroySliders();
        }
      }, 1000));

      window.addEventListener('orientationchange', function() {
        var innerWidthAfterEvent = window.innerHeight;
        if (innerWidthAfterEvent >= sliderWidthThreshold) {
          createSliders();
        } else {
          // Remove interval
          clearInterval(autoMoveIntervalMain);
          // Remove sliders and restore markup
          destroySliders();
        }
      });
    }, 1500)
  })
</script>
<script>
  if (typeof debounce !== 'function') {
    function debounce(func, wait, immediate) {
      var timeout;
      return function () {
        var args = arguments;
        var later = function () {
          timeout = null;
          if (!immediate) func.apply(this, args);
        }
        var callNow = immediate && !timeout;
        clearTimeout(timeout);
        timeout = setTimeout(later, wait);
        if (callNow) func.apply(this, args);
      }
    }
  }

  window.addEventListener('load', function() {
    // Fit the height of the events tab to the impressions slider
    var impressionBoxWidthThreshold = 970;
    var events = document.querySelector('.events');
    var impressionsSlider = document.querySelector('.impressions');
    if (events !== null && typeof events !== 'undefined') {
      if (window.innerWidth > impressionBoxWidthThreshold) {
        events.style.overflowY = 'auto'
        events.style.height = impressionsSlider.offsetHeight + 'px';
      }
    }
    // Oh and on resize fit the size again
    window.addEventListener('resize', debounce(function() {
      if (events !== null && typeof events !== 'undefined') {
        if (window.innerWidth > impressionBoxWidthThreshold) {
          events = document.querySelector('.events');
          impressionsSlider = document.querySelector('.impressions');
          events.style.overflowY = 'scroll'
          events.style.height = impressionsSlider.offsetHeight + 'px';
        } else {
          events.style.removeProperty('height')
          events.style.removeProperty('overflow-y')
        }
      }
    }, 1000));
  });
</script>
